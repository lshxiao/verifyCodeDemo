.input-box{
  position: relative;
  width: 405rpx;
  height: 105rpx;
  margin: 30rpx auto;
}
.title{
  padding: 50rpx 0 0;
  font-size: 36rpx;
  color: #333;
  text-align: center;
}
.input-canvas{
  position: absolute;
  z-index: 10;
  width: 400rpx; 
  height: 100rpx; 
  background: url('data:image/jpg;base64,') no-repeat; 
  background-size: cover;
}
.input-box input{
  position: absolute;
  left: 10rpx;
  top: 10rpx;
  z-index: 1;
  opacity: 0;
  color: transparent;
  font-size: 0rpx;
}